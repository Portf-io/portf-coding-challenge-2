{
  "name": "example-nextjs-legacy-pages",
  "version": "0.13.11",
  "private": true,
  "scripts": {
    "build": "npm run generate && next build",
    "bundle": "node scripts/bundle.js",
    "check": "tsc --pretty --noEmit",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/graphql-server/schema",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@apollo/server": "^4.10.0",
    "@as-integrations/next": "^3.0.0",
    "@prisma/client": "^5.8.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "next": "13.4.12",
    "nexus": "^1.3.0",
    "nexus-prisma": "^2.0.1",
    "prisma": "^5.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@parcel/watcher": "^2.4.0",
    "@types/node": "20.2.3",
    "@types/react": "18.2.8",
    "esbuild": "0.17.19",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "@graphql-codegen/client-preset": "4.1.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --transpile-only prisma/seed.ts"
  }
}
